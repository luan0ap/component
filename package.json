{
	"name": "peng-component",
	"version": "1.3.4",
	"description": "component use in fast mall",
	"homepage": "https://www.pipk.top",
	"main": "dist/index.js",
	"scripts": {
		"eslint": "eslint ./src --color --fix",
		"nodemon": "set NODE_ENV=dev&& nodemon ./app.js --config nodemonrc.json",
		"test": "webpack --color --watch --env.NODE_ENV=dev --progress",
		"t": "parcel test/index.html",
		"w": "rollup --config build/rollup.config.dev.js -w",
		"b": "rollup --config build/rollup.config.dev.js",
		"p": "rollup --config build/rollup.config.prod.js",
		"parcel": "parcel build test/index.html --public-url ./"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pengliheng/component.git"
	},
	"keywords": [
		"component"
	],
